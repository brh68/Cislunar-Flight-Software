<interface name="RTCPort" namespace="Ref">
    	<comment>
    	port to talk to RTC, returns a value
    	</comment>
    	<args>
        	<arg name="time" type="U32">
		</arg>
        	<arg name="operation" type="ENUM">
            		<enum name="RTCOp">
                		<item name="SET_TIME"/>
                		<item name="GET_TIME"/>
            		</enum>
        	</arg>
    	</args>
	<return type="U32" pass_by="value"/>
</interface>

